{
  "name": "cas-fee-projekt-1-starter-template",
  "version": "1.0.0",
  "description": "Testat template",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "w3c": "html-validator --file ./source/public/index.html  --verbose",
    "eslint": "eslint --fix --ext .js './source/public/**/*.js'",
    "stylelint": "stylelint -f 'verbose' './source/public/**/*.css' --fix",
    "all": "npm run stylelint && npm run w3c  && npm run eslint  && node -e \"console.log('npm run all completed')\""
  },
  "author": "Fabrice Tobler",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-validator-cli": "^7.0.1",
    "prettier": "2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "express": "^4.18.0"
  }
}
